<%= title %>

<%= '=' * title.size %>

<% notes.each do |label, per_file| %>

<%= label %> 
<%= '-' * (label.size) %>

<%   per_file.each do |file, line_notes| %>

### file://<%= file %>

<% line_notes.sort!{ |a,b| a[0] <=> b[0] } %><% line_notes.each do |line, note| %>
* <%= note %> (<%= line %>)
<% end; end; end %>

